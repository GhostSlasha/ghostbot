client.on('message', message => {
	if (message.content === '?cleansetting') {
		message.channel.send('```amp 20 then go to sc4 and put 0.1242, 123, 672.131, -3.54, 18.23, 2.424, 16.42 then go to bass in treble and put the bass at -10 treble at -5 volume 10```');

	}
	else if(message.content === '?help') {
		message.channel.send('```coming soon...```');
	}
	else if(message.content === '?decal') {
		message.channel.send('https://pastebin.com/ffTZq4Wq');

	}else if(message.content === '?decal2') {
		message.channel.send('https://pastebin.com/1ziqa1t0');
	}
	else if (message.content == '?news') {
		message.channel.send('bypassed code method will be added');
	}
	else if (message.content == '?beammethod') {
		message.channel.send('#har-method');
	}
	else if (message.content == '?nextupdate') {
		message.channel.send('next month');
	}
	else if (message.content == 'imsobored') {
